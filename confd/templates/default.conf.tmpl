{{range $dir := lsdir "/"}}
server {
    listen {{$custdir := printf "/%s/port" $dir}}{{getv $custdir}};
    server_name {{$custdir := printf "/%s/server_name" $dir}}{{getv $custdir}};

    {{$custdir := printf "/%s/location/*" $dir}}{{range  gets $custdir}}
    location {{$data := json .Value}}{{$data.LOCATION}} {
         proxy_pass  http://{{$data := json .Value}}{{$data.PROXY_PASS}};
         proxy_set_header    X-Forwarded-For    $proxy_add_x_forwarded_for;
         proxy_set_header    X-Real-IP    $remote_addr;
         proxy_set_header    Host    $host;
         proxy_http_version    1.1;
    {{end}}
         }

    }
{{end}}

